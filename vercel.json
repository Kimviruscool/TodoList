{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "build.gradle",
      "use": "@vercel/gradle"
    }
  ],
  "routes": [
    {
      "src": "/CSS/(.*)",
      "dest": "/CSS/$1"
    },
    {
      "src": "/JS/(.*)",
      "dest": "/JS/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "SPRING_PROFILES_ACTIVE": "production",
    "PORT": "8080"
  }
}
